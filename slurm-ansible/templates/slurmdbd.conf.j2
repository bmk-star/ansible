#
# slurmdbd.conf generated by Ansible
#

ArchiveEvents=yes
ArchiveJobs=yes
ArchiveResvs=yes
ArchiveSteps=yes
ArchiveSuspend=yes
ArchiveTXN=yes
ArchiveUsage=yes

StorageType=accounting_storage/mysql
StorageHost=127.0.0.1
StorageUser={{ slurm_db_user }}
StoragePass={{ slurm_db_pass }}
StorageLoc=slurm_acct_db

SlurmUser=slurm
DbdPort=6819
DbdHost=localhost
PidFile=/var/run/slurmdbd.pid
LogFile=/var/log/slurm/slurmdbd.log
DebugLevel=info
